<div id="wrapper">
  <h1>Articles</h1>
  <h2><i>You can save and also add notes to articles by clicking on them.</i></h2>
  <div id="articles">

    <div class="card">
      {{#each articles}}
      <div class="card-body">
        <img src="{{image}}">
        <h5 class="card-title" data-id="{{_id}}">{{title}}</h5>
        <a href="{{link}}" class="btn btn-primary">Article Link</a>
        <button  data-id="{{_id}}" class=" SaveArticles btn btn-primary ">Save Article</button>
      </div>
      {{/each}}
    </div>
  </div>
  <div id="notes"></div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="app.js">



</script>